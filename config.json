{
    "providers": ["desktop_commander_mcp", "anthropic_api", "openai_api", "hardcoded_fallback"],
    "logging_level": "INFO",
    "enable_memory": true,
    
    "performance": {
        "max_concurrent_requests": 10,
        "provider_timeout": 30,
        "circuit_breaker": {
            "failure_threshold": 3,
            "timeout_minutes": 5
        },
        "retry_schedule": [1, 2, 4]
    },
    
    "features": {
        "enable_neural_health_monitor": true,
        "enable_circuit_breaker": true,
        "enable_retry_logic": true
    },
    
    "neural_context": {
        "max_tokens": 1000,
        "max_memories": 10,
        "compression_enabled": true
    },
    
    "health_monitoring": {
        "cleanup_interval": 600,
        "memory_size_limit": 500000,
        "archive_threshold": 0.8
    }
}